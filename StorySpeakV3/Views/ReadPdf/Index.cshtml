<h1>PDF Upload and MP3 Player</h1>


<!-- PDF Upload Section -->
<h2>Upload PDF</h2>

<form enctype="multipart/form-data" method="post" asp-controller="ReadPdf" asp-action="ExtractText">
    <input type="file" name="pdfFile" accept=".pdf" required />
    <button type="submit">Upload PDF</button>
</form>


